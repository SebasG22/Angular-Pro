{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "content-projection": {
      "root": "apps/content-projection/",
      "sourceRoot": "apps/content-projection/src",
      "projectType": "application",
      "prefix": "angpro",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/content-projection",
            "index": "apps/content-projection/src/index.html",
            "main": "apps/content-projection/src/main.ts",
            "polyfills": "apps/content-projection/src/polyfills.ts",
            "tsConfig": "apps/content-projection/tsconfig.app.json",
            "assets": [
              "apps/content-projection/src/favicon.ico",
              "apps/content-projection/src/assets"
            ],
            "styles": [
              "apps/content-projection/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/content-projection/src/environments/environment.ts",
                  "with": "apps/content-projection/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "content-projection:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "content-projection:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "content-projection:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/content-projection/tsconfig.app.json",
              "apps/content-projection/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "apps/content-projection/jest.config.js",
            "tsConfig": "apps/content-projection/tsconfig.spec.json",
            "setupFile": "apps/content-projection/src/test-setup.ts"
          }
        }
      }
    },
